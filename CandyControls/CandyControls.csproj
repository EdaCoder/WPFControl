<Project Sdk="Microsoft.NET.Sdk">

	<PropertyGroup>
		<TargetFramework>net8.0-windows7.0</TargetFramework>
		<GenerateAssemblyInfo>false</GenerateAssemblyInfo>
		<UseWPF>true</UseWPF>
		<GeneratePackageOnBuild>false</GeneratePackageOnBuild>
		<Authors>EmilyEdna</Authors>
		<Description>XExten对应的WPFUI套件包</Description>
		<GeneratePackageOnBuild>true</GeneratePackageOnBuild>
		<PackageId>XExten.WPFControls</PackageId>
		<Product>XExten.WPFControls</Product>
		<Version>1.0.0</Version>
		<AssemblyVersion>1.0.0</AssemblyVersion>
		<PackageProjectUrl>https://github.com/EdaCoder/WPFControl</PackageProjectUrl>
		<Copyright>EdnaCoder Oganization</Copyright>
		<FileVersion>1.0.0</FileVersion>
	</PropertyGroup>
	<ItemGroup>
		<PackageReference Include="Microsoft.AspNet.WebApi.Client" Version="6.0.0" />
		<PackageReference Include="SkiaImageView.Wpf" Version="1.5.0" />
		<PackageReference Include="System.Drawing.Common" Version="8.0.3" />
		<PackageReference Include="XExten.Advance" Version="2.2.2.2" />
	</ItemGroup>
</Project>
